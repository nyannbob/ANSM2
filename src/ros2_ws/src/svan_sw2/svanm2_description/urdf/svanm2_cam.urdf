      <sensor type="depth" name="realsense_D435">
        <always_on>true</always_on>
        <update_rate>30.0</update_rate>

        <visualize>true</visualize>
        <camera>
            <horizontal_fov>1.3439</horizontal_fov>
            <image>
                <width>1920</width>
                <height>1080</height>
                <format>R8G8B8</format>
            </image>
